        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MigrationConvertor"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="925">
    <Grid>
        <TextBox Height="32" HorizontalAlignment="Left" Margin="181,10,0,0" Name="FileNameTextBox"
                 VerticalAlignment="Top" Width="393" IsReadOnly="true"/>
        <Button Content="Browse" Height="32" HorizontalAlignment="Left" Margin="622,10,0,0"
                Name="button1" VerticalAlignment="Top" Width="88" Click="buttonUpload_Click" />
        <TextBox x:Name="textBoxOutput" IsReadOnly="true" HorizontalAlignment="Left" Height="535" Margin="10,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="886" TextChanged="textBoxOutput_TextChanged"/>
    </Grid>
